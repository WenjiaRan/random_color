{
  "name": "Random Text Color",
  "version": "1.0",
  "description": "Renders a random color to each text on a web page",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}